-X POST \
-H 'Accept: application/json; charset=utf-8' \
-H 'Content-Type: application/json; charset=utf-8' \
-H 'Authorization: Api-Token ACCESS_TOKEN' \
-d $'[{"schemaId":"builtin:bizevents.http.incoming","schemaVersion":"1.0.2","scope":"environment","value":{"enabled":true,"ruleName":"[ws] easyTravel - /login","triggers":[{"source":{"dataSource":"request.path"},"type":"EQUALS","value":"/login","caseSensitive":false}],"event":{"provider":{"sourceType":"constant.string","source":"www.easytravel.com"},"type":{"sourceType":"constant.string","source":"com.easytravel.login"},"category":{"sourceType":"request.path"},"data":[{"name":"password","source":{"sourceType":"request.body","path":"password"}},{"name":"lastName","source":{"sourceType":"request.body","path":"lastName"}},{"name":"firstName","source":{"sourceType":"request.body","path":"user.firstName"}},{"name":"email","source":{"sourceType":"request.body","path":"email"}}]}}}]'
